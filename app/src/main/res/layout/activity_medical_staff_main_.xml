<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.safetylifeproperty.slpro.slp.medical_staff_main_Activity">
    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="13dp"
        android:textStyle="bold"
        android:textSize="30dp"
        android:textColor="#c9e6d8"
        android:text="* 의료진용"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="60dp"
        android:background="#c7e2f6"
        android:id="@+id/relativeLayout9">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:id="@+id/relativeLayout11"
            android:background="#c7e2f6">

            <RelativeLayout
                android:background="#ffffff"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:id="@+id/relativeLayout6">
                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:textSize="15dp"
                    android:text="순번" />


            </RelativeLayout>

            <RelativeLayout
                android:background="#ffffff"
                android:layout_width="80dp"
                android:layout_height="100dp"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/relativeLayout6"
                android:layout_marginStart="10dp"
                android:id="@+id/relativeLayout7">
                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:textSize="15dp"
                    android:text="Name" />
            </RelativeLayout>

            <RelativeLayout
                android:background="#ffffff"
                android:layout_width="110dp"
                android:layout_height="100dp"
                android:layout_marginStart="9dp"
                android:id="@+id/relativeLayout8"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/relativeLayout7">
                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:textSize="15dp"
                    android:text="I D" />
            </RelativeLayout>

            <RelativeLayout
                android:background="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginStart="12dp"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/relativeLayout8"
                android:id="@+id/relativeLayout10">
                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:textSize="15dp"
                    android:text="Open" />
            </RelativeLayout>




            </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="2dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:backgroundTint="#c7e2f6"
            android:layout_below="@+id/relativeLayout11"
            android:background="#ffffff"
            android:id="@+id/relativeLayout14">
            <ScrollView
                android:id="@+id/main_box_scrollview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#c7e2f6"
                android:visibility="visible"
                android:scrollbars="none"
                android:fillViewport="true">

                <ExpandableListView
                    android:id="@+id/elv"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:groupIndicator="@null"></ExpandableListView>
            </ScrollView>
        </RelativeLayout>


    </RelativeLayout>

    <RelativeLayout
        android:background="#008ed3"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true">


        <Button
            android:id="@+id/button6"
            android:background="#54d7cc"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="17dp"
            android:layout_margin="10dp"
            android:textSize="15dp"
            android:layout_weight="0.5"
            android:textColor="#ffffff"
            android:textStyle="bold"
            android:text="환자 등록"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true" />

        <Button
            android:id="@+id/button11"
            android:layout_weight="0.5"
            android:layout_margin="10dp"
            android:background="#54d7cc"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:textSize="15dp"
            android:textColor="#ffffff"
            android:textStyle="bold"
            android:text="환자 수정"
            android:layout_alignBaseline="@+id/button6"
            android:layout_alignBottom="@+id/button6"
            android:layout_centerHorizontal="true" />

        <Button
            android:id="@+id/button13"
            android:background="#54d7cc"
            android:layout_margin="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:textSize="15dp"
            android:layout_weight="0.5"
            android:textStyle="bold"
            android:textColor="#ffffff"
            android:text="환자 삭제"
            android:layout_marginEnd="27dp"
            android:layout_alignBaseline="@+id/button11"
            android:layout_alignBottom="@+id/button11"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>
</RelativeLayout>
